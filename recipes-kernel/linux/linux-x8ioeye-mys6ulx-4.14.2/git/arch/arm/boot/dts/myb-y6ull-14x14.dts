&ecspi4 {
	compatible = "fsl,imx6ul-ecspi";
	fsl,spi-num-chipselects = <1>;
	cs-gpios = <&gpio3 26 0>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi4 &pinctrl_ecspi4_cs>;
	status = "okay";
	spidev@0x00{
		#address-cellss=<1>;
		#size-cells=<1>;
		compatible = "rohm,dh2228fv";
		spi-max-frequency = <8000000>;
		reg = <0>;
	};
};


