&ecspi1 {
	compatible = "fsl,imx6ul-ecspi";
	fsl,spi-num-chipselects = <1>;
	cs-gpios = <&gpio4 26 0>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi1 &pinctrl_ecspi1_cs>;
	status = "okay";
	spidev@0x00{
		#address-cellss=<1>;
		#size-cells=<1>;
		compatible = "rohm,dh2228fv";
		spi-max-frequency = <8000000>;
		reg = <0>;
	};
};
